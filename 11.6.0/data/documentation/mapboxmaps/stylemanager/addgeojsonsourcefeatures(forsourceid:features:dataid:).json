t","text":" "},{"type":"text","text":"call, the data ID in the "},{"type":"codeVoice","code":"source-data-loaded"},{"type":"text","text":"event will be null."}]}]},{"type":"aside","name":"Throws","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""},{"identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleError","type":"reference","isActive":true},{"text":" if there is a problem adding features to the source.","type":"text"}]}],"style":"note"}]}],"metadata":{"externalID":"s:10MapboxMaps12StyleManagerC24addGeoJSONSourceFeatures11forSourceId8features04dataK0ySS_Say4Turf7FeatureVGSSSgtF","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"addGeoJSONSourceFeatures","kind":"identifier"},{"kind":"text","text":"("},{"text":"forSourceId","kind":"externalParam"},{"kind":"text","text":": "},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":", ","kind":"text"},{"text":"features","kind":"externalParam"},{"kind":"text","text":": ["},{"preciseIdentifier":"s:10MapboxMaps7Featurea","text":"Feature","kind":"typeIdentifier"},{"text":"], ","kind":"text"},{"kind":"externalParam","text":"dataId"},{"kind":"text","text":": "},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":"?)","kind":"text"}],"role":"symbol","roleHeading":"Instance Method","symbolKind":"method","modules":[{"name":"MapboxMaps"}],"title":"addGeoJSONSourceFeatures(forSourceId:features:dataId:)"},"abstract":[{"type":"text","text":"Add additional features to a GeoJSON style source."}],"identifier":{"url":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleManager\/addGeoJSONSourceFeatures(forSourceId:features:dataId:)","interfaceLanguage":"swift"},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxmaps\/stylemanager\/addgeojsonsourcefeatures(forsourceid:features:dataid:)"]}],"hierarchy":{"paths":[["doc:\/\/MapboxMaps\/documentation\/MapboxMaps","doc:\/\/Mapbox