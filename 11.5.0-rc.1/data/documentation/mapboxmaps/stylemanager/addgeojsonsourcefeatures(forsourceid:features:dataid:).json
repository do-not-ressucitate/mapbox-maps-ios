{"abstract":[{"type":"text","text":"Add additional features to a GeoJSON style source."}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleManager\/addGeoJSONSourceFeatures(forSourceId:features:dataId:)"},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/MapboxMaps\/documentation\/MapboxMaps","doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/Style","doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleManager"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"platforms":["iOS"],"tokens":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"addGeoJSONSourceFeatures","kind":"identifier"},{"kind":"text","text":"("},{"text":"forSourceId","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"sourceId","kind":"internalParam"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"text":", ","kind":"text"},{"text":"features","kind":"externalParam"},{"kind":"text","text":": ["},{"preciseIdentifier":"s:10MapboxMaps7Featurea","text":"Feature","identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/Feature-swift.typealias","kind":"typeIdentifier"},{"text":"], ","kind":"text"},{"text":"dataId","kind":"externalParam"},{"kind":"text","text":": "},{"text":"String","preciseIdentifier":"s:SS","kind":"typeIdentifier"},{"kind":"text","text":"? = nil)"}]}]},{"kind":"parameters","parameters":[{"name":"sourceId","content":[{"inlineContent":[{"type":"text","text":"The identifier of the style source."}],"type":"paragraph"}]},{"name":"features","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"An array of GeoJSON features to be added to the source."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"An arbitrary string used to track the given GeoJSON data.","type":"text"}]}],"name":"dataId"}]},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","level":2,"type":"heading"},{"type":"