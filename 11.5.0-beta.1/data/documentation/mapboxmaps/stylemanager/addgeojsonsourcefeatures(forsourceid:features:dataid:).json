eVoice"},{"text":"event will be null.","type":"text"}]}],"name":"Note","type":"aside","style":"note"},{"style":"note","name":"Throws","type":"aside","content":[{"inlineContent":[{"type":"text","text":""},{"identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleError","isActive":true,"type":"reference"},{"type":"text","text":" if there is a problem adding features to the source."}],"type":"paragraph"}]}]}],"metadata":{"title":"addGeoJSONSourceFeatures(forSourceId:features:dataId:)","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"addGeoJSONSourceFeatures","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"externalParam","text":"forSourceId"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:SS","kind":"typeIdentifier","text":"String"},{"text":", ","kind":"text"},{"text":"features","kind":"externalParam"},{"kind":"text","text":": ["},{"text":"Feature","kind":"typeIdentifier","preciseIdentifier":"s:10MapboxMaps7Featurea"},{"text":"], ","kind":"text"},{"text":"dataId","kind":"externalParam"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:SS","text":"String"},{"kind":"text","text":"?)"}],"symbolKind":"method","role":"symbol","externalID":"s:10MapboxMaps12StyleManagerC24addGeoJSONSourceFeatures11forSourceId8features04dataK0ySS_Say4Turf7FeatureVGSSSgtF","roleHeading":"Instance Method","modules":[{"name":"MapboxMaps"}]},"identifier":{"url":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleManager\/addGeoJSONSourceFeatures(forSourceId:features:dataId:)","interfaceLanguage":"swift"},"schemaVersion":{"minor":3,"major":0,"patch":0},"variants":[{"paths":["\/documentation\/mapboxmaps\/stylemanager\/addgeojsonsourcefeatures(forsourceid:features:dataid:)"],"traits":[{"interfaceLanguage":"swift"}]}],"hierarchy":{"paths":[["doc:\/\/MapboxMaps\/documentation\/MapboxMaps","doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/Style","doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/StyleManager"]]},"kind":"symbol